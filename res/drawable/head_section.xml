<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <gradient
                android:angle="360"
                android:centerColor="#5f5f61"
                android:endColor="#1f2121"
                android:gradientRadius="360"
                android:startColor="#1f2121"
                android:type="linear" />
            <stroke
                android:width="1dp"
                android:color="#212121" />

        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle" >
            <solid android:color="#111215" />
        </shape>
    </item>

</layer-list>


